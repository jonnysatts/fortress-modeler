{
  "name": "fortress-financial-modeler",
  "productName": "Fortress Financial Modeler",
  "description": "Professional financial modeling and analysis tool",
  "author": "Fortress Financial",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "predev": "node scripts/check-deps.cjs",
    "prebuild": "node scripts/check-deps.cjs",
    "prelint": "node scripts/check-deps.cjs",
    "verify": "node scripts/verify-requirements.cjs",
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite preview --host 0.0.0.0 --port $PORT",
    "typecheck": "tsc --noEmit",
    "cache-bust": "echo 'Force re-build'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:8081 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "dist": "npm run build && electron-builder --publish=never",
    "dist:win": "npm run build && electron-builder --win --publish=never",
    "release": "npm run build && electron-builder --publish=always",
    "setup:electron": "npm install electron electron-builder electron-updater concurrently wait-on --save-dev && node scripts/create-icons.cjs",
    "icons:create": "node scripts/create-icons.cjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
