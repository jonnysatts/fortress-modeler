# Fortress Financial Modeler - Environment Configuration Template
# Copy this file to .env and fill in your actual values

# AI API Keys (Optional - for development tools)
GEMINI_API_KEY=your-gemini-api-key-here
OPENAI_API_KEY=your-openai-api-key-here
CODEGPT_API_KEY=your-codegpt-api-key-here

# Supabase Configuration (Required for cloud mode)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Backend Mode (CRITICAL - set to true for cloud projects)
VITE_USE_SUPABASE_BACKEND=true

# OAuth Configuration (Required for authentication)
VITE_GOOGLE_CLIENT_ID=your-google-client-id.googleusercontent.com

# Debug Mode (Optional)
VITE_DEBUG=false